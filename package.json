{
  "name": "camomile",
  "version": "0.0.0",
  "description": "Node.js HTTP image proxy to route images through SSL",
  "keywords": [
    "ssl",
    "node",
    "proxy",
    "camo",
    "camomile",
    "security",
    "unified"
  ],
  "homepage": "https://unifiedjs.com",
  "bugs": "https://github.com/rehypejs/camomile/issues",
  "repository": "rehypejs/camomile",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "license": "MIT",
  "author": "Merlijn Vos <merlijn@soverin.net>",
  "type": "module",
  "exports": "./index.js",
  "files": [
    "lib/",
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "build": "npm run build-clean && tsc --build",
    "build-clean": "tsc --build --clean",
    "format": "remark . -qfo && prettier . -w --log-level warn",
    "test": "npm run build && npm run format && npm run test-api",
    "test-api": "node --test test.js"
  },
  "prettier": {
    "bracketSpacing": false,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-wooorm"
    ]
  },
  "dependencies": {
    "ipaddr.js": "^2.0.0",
    "undici": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "prettier": "^3.0.0",
    "remark-cli": "^11.0.0",
    "remark-preset-wooorm": "^9.0.0",
    "typescript": "^5.0.0"
  }
}
